{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": false,
    "target": "es5",
    "jsx": "react",
    "lib": [
      "dom",
      "es5",
      "es6"
    ],
    "allowSyntheticDefaultImports": true,
    "importHelpers": true
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
